{
    "name": "wamesk/laravel-nova-unit",
    "description": "Laravel Nova 4 Units.",
    "version": "2.1",
    "keywords": [
        "laravel",
        "nova",
        "nova4",
        "unit",
        "field"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "WAME s.r.o.",
            "homepage": "https://wame.sk"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "laravel/framework": "9.*|10.*|11.*",
        "laravel/nova": "^4.0",
        "sakanjo/laravel-easy-enum": "v1.*"
    },
    "autoload": {
        "psr-4": {
            "Wame\\LaravelNovaUnit\\": "src/"
        },
        "files": [
            "src/Utils/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wame\\LaravelNovaUnit\\Providers\\PackageServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
